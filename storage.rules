
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
     // Regla de seguridad predeterminada: solo los usuarios autenticados pueden leer y escribir.
     // Ajusta esto según las necesidades de tu aplicación.
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
